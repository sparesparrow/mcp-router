{
  "name": "mcp-router",
  "version": "0.1.0",
  "description": "Model Context Protocol Router for managing multiple MCP servers",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --copy-files",
    "start": "node dist/index.js",
    "dev": "ts-node --transpile-only src/index.ts",
    "watch": "nodemon --exec ts-node --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "router",
    "model",
    "context",
    "protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mermaid": "^10.6.0",
    "node-fetch": "^2.6.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.8.3",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.17.22",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
